<?xml version="1.0" ?>
<robot name="cabinet">
  <link name="world"/>
  <joint name="cabinet_transform" type="fixed">
    <parent link="world"/>
    <child link="cabinet"/>
    <origin rpy="0 0 0" xyz="0 0 0.2"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="cabinet">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual name="cabinet_visual">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="hingecabinet.stl"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="hingecabinet.stl"/>
      </geometry>
    </collision>
  </link>
  <!-- Left Door -->
  <joint name="cabinet_door_left_joint_fixed" type="fixed">
    <parent link="cabinet"/>
    <child link="cabinet_door_left_anchor_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
  <link name="cabinet_door_left_anchor_link"/>
  <joint name="cabinet_door_left_joint" type="revolute">
    <parent link="cabinet_door_left_anchor_link"/>
    <child link="cabinet_door_left_link_tmp"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-1.57" upper="0" velocity="100"/>
  </joint>
  <link name="cabinet_door_left_link_tmp"/>
  <joint name="cabinet_door_left_joint_fixed_2" type="fixed">
    <parent link="cabinet_door_left_link_tmp"/>
    <child link="cabinet_door_left_link"/>
    <origin rpy="0 0 0" xyz="0.18 -0.30 0.0"/>
  </joint>
  <link name="cabinet_door_left_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual name="cabinet_door_left_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="hingedoor.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="hingedoor.stl" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
  </link>
  <joint name="cabinet_door_left_handle_transform" type="fixed">
    <parent link="cabinet_door_left_link_tmp"/>
    <child link="cabinet_door_left_handle"/>
    <origin rpy="0 0 0" xyz="0.02 -0.31 0.00"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="cabinet_door_left_handle">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
    <contact>
      <lateral_friction value="200.0"/>
      <spinning_friction value="0.0"/>
      <rolling_friction value="0.0"/>
    </contact>
    <visual name="cabinet_door_left_handle_visual">
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="hingehandle.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="hingehandle.stl" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
  </link>
</robot>
