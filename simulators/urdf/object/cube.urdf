<?xml version="1.0" ?>
<robot name="cube">
  <link name="baseLink">
    <contact>
      <friction_anchor/>
      <lateral_friction value="2.0"/>
      <rolling_friction value="0.0"/>
      <stiffness value="5000"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.10"/>
      <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	    <mesh filename="cube.obj" scale="0.05 0.05 0.05"/>
      </geometry>
       <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 	<box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
  </link>
</robot>

